{
  "name": "nextapp",
  "version": "0.1.0",
  "private": true,
  "repository": ".",
  "scripts": {
    "dev": "next dev",
    "dev:ts": "npm run dev",
    "ts-compile": "tsc --noEmit",
    "ts:watch": "eslint ./**/*.{ts,tsx} --watch",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "cd .. && node -e \"if (process.env.NODE_ENV === 'development'){process.exit(1)} \" || husky install frontend/.husky",
    "lint-staged": "lint-staged",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --color --fix --max-warnings=0"
    ]
  },
  "dependencies": {
    "@google-cloud/error-reporting": "3.0.5",
    "@google-cloud/logging-winston": "6.0.0",
    "@svgr/webpack": "8.1.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "classnames": "2.5.1",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "luxon": "3.6.1",
    "next": "^15.3.0",
    "next-i18n-router": "^5.5.1",
    "next-nprogress-bar": "^2.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@next/bundle-analyzer": "15.3.0",
    "@next/eslint-plugin-next": "^15.3.0",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/lodash": "^4.17.17",
    "@types/luxon": "3.6.2",
    "@types/node": "22.14.0",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "cross-env": "7.0.3",
    "eslint": "^9.24.0",
    "eslint-config-next": "15.3.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "husky": "9.1.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "tailwindcss": "^4.1.3",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.29.1"
  }
}
