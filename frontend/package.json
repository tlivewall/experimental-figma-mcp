{
  "name": "nextapp",
  "version": "0.1.0",
  "private": true,
  "repository": ".",
  "scripts": {
    "dev": "next dev --experimental-https",
    "dev:ts": "npm run dev",
    "ts-compile": "tsc --noEmit",
    "ts:watch": "eslint ./**/*.{ts,tsx} --watch",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "cd .. && node -e \"if (process.env.NODE_ENV === 'development'){process.exit(1)} \" || husky install frontend/.husky",
    "lint-staged": "lint-staged",
    "analyze": "cross-env ANALYZE=true next build",
    "storyblok:setup-quick-actions": "node scripts/create-quick-actions-storyblok.js"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --color --fix --max-warnings=0"
    ]
  },
  "dependencies": {
    "@google-cloud/error-reporting": "3.0.5",
    "@google-cloud/logging-winston": "6.0.1",
    "@storyblok/js": "^4.3.0",
    "@storyblok/react": "^5.4.12",
    "@storyblok/richtext": "^3.8.0",
    "@svgr/webpack": "8.1.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "classnames": "2.5.1",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "luxon": "3.7.2",
    "next": "^15.5.4",
    "next-i18n-router": "^5.5.3",
    "next-nprogress-bar": "^2.4.7",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "storyblok-js-client": "^7.1.4",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@next/bundle-analyzer": "15.5.4",
    "@next/eslint-plugin-next": "^15.5.4",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/lodash": "^4.17.20",
    "@types/luxon": "3.7.1",
    "@types/node": "24.5.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "cross-env": "10.0.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "husky": "9.1.7",
    "lint-staged": "^16.2.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.44.1"
  }
}
